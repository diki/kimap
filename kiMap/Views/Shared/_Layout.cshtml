<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/normalize.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/landing_header.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Content/ie.css")" rel="stylesheet" type="text/css" />
    <!--[if IE]>
    <link rel="stylesheet" href="../../Content/style/ie/all.css" type="text/css" />
    <![endif]-->
    <!--[if IE 7]>
    <link rel="stylesheet" href="../../Content/style/ie/7.css" type="text/css" />
    <![endif]-->
    <!--[if IE 8]>
    <link rel="stylesheet" href="../../Content/style/ie/8.css" type="text/css" />
    <![endif]-->
    <!--[if IE 9]>
    <link rel="stylesheet" href="../../Content/style/ie/9.css" type="text/css" />
    <![endif]-->
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/slides.min.jquery.js")" type="text/javascript"></script>
    <script src="../../Content/style/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
</head> 
<body>
    

    
    @{ Html.RenderPartial("FacebookInit"); }
    <div id="header" class="">
        <div id="headerTop">
            <div class="container_960">
                <div class="left-part">
                    <a class="brand" href="../" style="">
                        <img style="margin-top:11px;" src="../../Content/img/logottt.png" />
                    </a>
                </div>

                <div class="right-part">
                    <div id="logonPartial" class="logon-wrapper" style="">
                        <div style="margin-top: 8px;">
                            @{ Html.RenderPartial("_LogOnPartial"); }
                        </div>
                    </div>
                </div>
                <br style="clear:both" />
            </div>
        </div>
        <div id="nav">
        </div>
       
    </div>

    <div id="page_wrap" class="container" id="stage" style="">
     @RenderBody()
    </div>
</body>
</html>
