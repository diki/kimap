@{
    ViewBag.Title = "site";
}
<link href="@Url.Content("~/Content/user_site.css")" rel="stylesheet" type="text/css" />

<div id="editorWrapper" style="position: relative;">
</div>
<script type="text/template" id="header-widget-template">
        	<div class="content widget-header">
            	<h1><%=header%></h1>
            	<h2><%=subheader%></h2>
        	</div>
</script>
<script type="text/template" id="header-widget-alt-template">
        	<div class="content widget-header bg-noise1">
            	<h2><%=subheader%></h2>
            	<h1><%=header%></h1>
        	</div>
</script>
<script type="text/template" id="edit-widget-template">
        	<div class="edit-view">
            	<div class="right-part right-part-floater edit-view-button"><span>edit</span></div>
            	<br style="clear:right" />
            	
            	<% if(slider) { %>
            		<div class="right-part right-part-floater edit-view-slide-button ">right</div>
            		<div class="left-part left-part-floater edit-view-slide-button">left</div>
            		<br style="clear:both" />
            	<% } %>
            	
            	<div class="center-part content-center edit-view-add-button">
            		<div class="center-part">
	            		<span>+</span>
            		</div>
            	</div>
        	</div>
</script>
<script type="text/template" id="text-widget-template">
        	<div class="content widget-text-paragraph paragraph-image widget-imgtext height140">
            	<div class="right-part img-container">
            	</div>
            	<br style="clear:left	" />
            	<h2><%=title%></h2>
            	<div><%=content%></div>
        	</div>
</script>
<script type="text/template" id="text-widget-alt-template">
        	<div class="content widget-text-paragraph paragraph-image widget-imgtext height140">
            	<div class="left-part img-container">
            	</div>
            	<br style="clear:right;" />
            	<h2><%=title%></h2>
            	<div><%=content%></div>
        	</div>
</script>
<script type="text/template" id="text-widget-extra-template">
        	<div class="content widget-text-paragraph paragraph-image widget-imgtext">
            	<h2><%=title%></h2>
            	<div><%=content%></div>
        	</div>
</script>
<script type="text/template" id="title-widget-template">
        	<div class="content widget-text widget-title">
            	<h1 class="content-center"><%=content%></h1>
        	</div>
</script>
<script type="text/template" id="title-widget-alt-template">
        	<div class="content widget-text widget-title">
            	<h1><%=content%></h1>
        	</div>
</script>
<script type="text/template" id="gallery-widget-template">
        	<div class="content widget-text widget-gallery">
            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            		<h2><%=title1%></h2>
            		<h3><%=content1%></h3>
            	</div>

            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            		<h2><%=title2%></h2>
            		<h3><%=content2%></h3>
            	</div>

            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            		<h2><%=title3%></h2>
            		<h3><%=content3%></h3>
            	</div>
            	
            	<br style="clear: both;"/>
        	</div>
</script>
<script type="text/template" id="gallery-widget-alt-template">
        	<div class="content widget-text widget-gallery">
            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            	</div>
            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            	</div>
            	<div class="left-part span_3">
	            	<div class="img-container">
            		</div>
            	</div>
            	
            	<br style="clear: both;"/>
        	</div>
</script>
<script type="text/template" id="bio-widget-template">
        	<div class="content widget-text-paragraph paragraph-image widget-bio height140">
            	<div class="left-part left-part-floater span_4">
            		<div class="img-container">
            		</div>
            	</div>
            	
            	<div class="left-part left-part-floater span_12 bio-container">
	            	<h2><%=title%></h2>
	            	<% if(fb!==undefined) { %>
	            		<h4><%=fb%></h4>
	            	<% } %>
	            	<% if(tw!==undefined) { %>
	            		<h4><%=tw%></h4>
	            	<% } %>
	            	<ul class="bio-list">
		            	<li><%=address%></li>
		            	<li><%=phone%></li>
		            	<li><%=email%></li>
		            	<li><%=website%></li>
	            	</ul>
            	</div>
            	
            	<br style="clear:both;" />
        	</div>
</script>
<script src="../../Scripts/lib/json2.js" type="text/javascript"></script>
<script src="../../Scripts/lib/underscore.js" type="text/javascript"></script>
<script src="../../Scripts/lib/underscore.string.js" type="text/javascript"></script>
<script src="../../Scripts/lib/backbone.js" type="text/javascript"></script>

<script src="../../Scripts/init.js" type="text/javascript"></script>

<script src="../../Scripts/models/WidgetModel.js" type="text/javascript"></script>




<script src="../../Scripts/collections/WidgetsCollection.js" type="text/javascript"></script>

<script src="../../Scripts/views/EditorView.js" type="text/javascript"></script>
<script src="../../Scripts/views/WidgetView.js" type="text/javascript"></script>

<script src="../../Scripts/app.js" type="text/javascript"></script>
